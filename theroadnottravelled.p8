function _init()
cls(12)
t=0
i=0
speed=12
idle=16
walk=20
jump=24
spritetable=
	{{idle,16,4,12},
	 {walk,20,4,4},
	 {jump,24,8,4}
	}
	
end

function _update()
 if(i<16) then
  t+=1
 end
 if (t>speed-1) then
  i+=1
  t=0
 end
 if (i>15) then
  i=0
 end
 if (btn(0)) then
  i=0
 end
end

function _draw()
 cls(12)
 spr((idle+i),64,64,1,2)
end
